{
    "env": {
        "browser": true,
        "node": true
    },
    "extends": "eslint:recommended",
    "rules": {
        "linebreak-style": [
            2, // "error",
            "windows"
        ],
        "quotes": [
            2, // "error",
            "single"
        ],
        "semi": [
            2, // "error",
            "always"
        ],
        "no-console": [
            0 // "off"
        ],
        "no-fallthrough": [
            0 // was "error"
            //			{ "commentPattern": "Drop thro" }
        ],
        "no-warning-comments": [
            2, // "error",
            { "terms": [ "reference path" ], "location": "anywhere" }
        ]
    }
}